; sbc09 hardware

MMU_BASE	EQU	$FB00

MMU_CTL		EQU	MMU_BASE + $20
MMU_ACC_KEY	EQU	MMU_BASE + $21
MMU_TASK_KEY	EQU	MMU_BASE + $22
MMU_RTI		EQU	MMU_BASE + $23

MMU_MAP		EQU	MMU_BASE + $30

MMU_CTL_ENMMU	EQU	$01
MMU_CTL_MODE8	EQU	$02
MMU_CTL_PROT	EQU	$04

; 16k map entries
MMU_16_0	EQU	0x00
MMU_16_4	EQU	0x02
MMU_16_8	EQU	0x04
MMU_16_C	EQU	0x06
