MMU_ACC_KEY	EQU	MMU_BASE + $11
MMU_TASK_KEY	EQU	MMU_BASE + $12
MMU_RTI		EQU	MMU_BASE + $13

MMU_MAP		EQU	MMU_BASE + $20

MMU_CTL_ENMMU	EQU	$01
MMU_CTL_MODE8	EQU	$02

; 16k map entries
MMU_16_0	EQU	0x00
